LIGGGHTS (Version LIGGGHTS-PUBLIC 3.8.0, compiled 2023-04-16-15:58:31 by anmol, git commit unknown)
atom_style	hybrid granular dipole
boundary	f f f
newton		off
atom_modify	sort 10000 0

communicate	single vel yes
units		si
processors	1 1 1

variable reg_dim equal 10 #meters

region		reg block -${reg_dim} ${reg_dim} -${reg_dim} ${reg_dim} -${reg_dim} ${reg_dim} units box
region		reg block -10 ${reg_dim} -${reg_dim} ${reg_dim} -${reg_dim} ${reg_dim} units box
region		reg block -10 10 -${reg_dim} ${reg_dim} -${reg_dim} ${reg_dim} units box
region		reg block -10 10 -10 ${reg_dim} -${reg_dim} ${reg_dim} units box
region		reg block -10 10 -10 10 -${reg_dim} ${reg_dim} units box
region		reg block -10 10 -10 10 -10 ${reg_dim} units box
region		reg block -10 10 -10 10 -10 10 units box
create_box	1 reg
Created orthogonal box = (-10 -10 -10) to (10 10 10)
  1 by 1 by 1 MPI processor grid
neighbor	2 bin
neigh_modify	delay 0 every 1

hard_particles	yes


fix 		m1 all property/global youngsModulus peratomtype 6e6
fix 		m2 all property/global poissonsRatio peratomtype 0.278
fix 		m3 all property/global coefficientRestitution peratomtypepair 1 0.06
fix 		m4 all property/global coefficientFriction peratomtypepair 1 0.4
fix 		m6 all property/global cohesionEnergyDensity peratomtypepair 1 0.01

fix m7 all property/global magneticSusceptibility peratomtype 1


pair_style gran model hertz tangential history #cohesion sjkr
pair_coeff	* *

timestep	1e-10 # seconds

#index of simulations
# variable a loop 10
#radius of atoms
variable atom_rad equal 1.0 #meters
#diameter of atoms
variable atom_dia equal 2*${atom_rad}
variable atom_dia equal 2*1


create_atoms	1 single 0 0 0
Created 1 atoms
create_atoms	1 single 0 0 ${atom_dia}
create_atoms	1 single 0 0 2
Created 1 atoms
set		atom 1 diameter ${atom_dia}
set		atom 1 diameter 2
  1 settings made for diameter
set     atom 2 diameter ${atom_dia}
set     atom 2 diameter 2
  1 settings made for diameter

# Magnetic force is gauss (B mag field)
#                       x   y   z
fix	mag all magnetic    0   0   1

# Define loops

label loop
variable c loop 20 40
#separation between the atoms
variable sep equal ${c}*${atom_rad}/10
variable sep equal 20*${atom_rad}/10
variable sep equal 20*1/10
print "separation distance = ${sep}"
separation distance = 2

create_atoms	1 single 0 0 -${sep}
create_atoms	1 single 0 0 -2
Created 1 atoms
group           atom_j id 3
1 atoms in group atom_j

set     atom 3 diameter ${atom_dia}
set     atom 3 diameter 2
  1 settings made for diameter


